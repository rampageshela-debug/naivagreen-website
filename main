    left: 6px;
    opacity: 1;
}

/* Moon icon (right) */
.slider .moon {
    right: 6px;
    opacity: 0;
    transform: translateY(-50%) scale(0.8);
}

/* When dark mode is active */
body.dark-mode .slider .sun {
    opacity: 0;
    transform: translateY(-50%) scale(0.8);
}

body.dark-mode .slider .moon {
    opacity: 1;
    transform: translateY(-50%) scale(1);
}
const toggleSwitch = document.querySelector('.theme-switch input');

function switchTheme(e) {
    if (e.target.checked) {
        document.body.classList.add('dark-mode');
        localStorage.setItem('darkMode', 'enabled');
    } else {
        document.body.classList.remove('dark-mode');
        localStorage.setItem('darkMode', 'disabled');
    }
}

toggleSwitch.addEventListener('change', switchTheme);

// Load preference + icon state
window.onload = () => {
    const darkMode = localStorage.getItem('darkMode');
    const systemPrefersDark = window.matchMedia("(prefers-color-scheme: dark)").matches;

    if (darkMode === "enabled" || (darkMode === null && systemPrefersDark)) {
        document.body.classList.add('dark-mode');
        toggleSwitch.checked = true;
    }
};
/* Rotation animation for icons */
@keyframes rotateOut {
    0% { transform: translateY(-50%) rotate(0deg) scale(1); opacity: 1; }
    100% { transform: translateY(-50%) rotate(180deg) scale(0.4); opacity: 0; }
}

@keyframes rotateIn {
    0% { transform: translateY(-50%) rotate(-180deg) scale(0.4); opacity: 0; }
    100% { transform: translateY(-50%) rotate(0deg) scale(1); opacity: 1; }
}

/* Base state */
.slider .icon {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    font-size: 16px;
    transition: opacity 0.4s ease;
}

/* Initial Positions */
.slider .sun { left: 6px; }
.slider .moon { right: 6px; opacity: 0; transform: translateY(-50%) scale(0.8); }

/* Light Mode ‚Üí Sun visible */
body:not(.dark-mode) .sun {
    animation: rotateIn 0.5s forwards;
}
body:not(.dark-mode) .moon {
    animation: rotateOut 0.5s forwards;
}

/* Dark Mode ‚Üí Moon visible */
body.dark-mode .sun {
    animation: rotateOut 0.5s forwards;
}
body.dark-mode .moon {
    animation: rotateIn 0.5s forwards;
}
